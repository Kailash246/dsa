   "source": [
    "import numpy as np\n",
    "x=np.array([1,2,3,4,5])\n",
    "def prod(x):\n",
    "    n=len(x)\n",
    "    res=[1]*n\n",
    "    for i in range(n):\n",
    "        for j in range(n):\n",
    "            if i!=j:\n",
    "                res[i]*=x[j]\n",
    "    return res\n",
    "\n",
    "result=prod(x)\n",
    "print(result)"
   ]
